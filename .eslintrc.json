{
	"env": {
		"es2021": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/no-unused-vars": "off",
		"no-console": "off"
	}
}
